{
  "version": 2,
  "builds": [
    { "src": "/app.py", "use": "@vercel/python@3.9" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/app.py" }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": { "app": { "runtime": "vercel-python@3.9", "memory": 512 } }
}
